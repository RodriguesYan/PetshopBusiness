@{
    ViewData["Title"] = "Home Page";
}
<link href="~/css/Home/Home.css" rel="stylesheet" />

<section class="main-content-layout">
    <section class="main-carousel">
        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="5"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="~/images/Index/Carousel/Home_TV_Alimentos_1900x300-2.png" style="height:300px;width: 100%;"  />
                </div>
                <div class="carousel-item">
                    <img src="~/images/Index/Carousel/Home_TV_Banho_1900x300.png" style="height:300px;width: 100%;" />
                </div>
                <div class="carousel-item">
                    <img src="~/images/Index/Carousel/Home_TV_desktop_agendamento_vf3.png" style="height:300px;width: 100%;"  />
                </div>
                <div class="carousel-item">
                    <img src="~/images/Index/Carousel/RETIRA-DESKTOP-05-01.png" style="height:300px;width: 100%;" />
                </div>
                <div class="carousel-item">
                    <img src="~/images/Index/Carousel/web_home_tv_10_off_app_UD.png" style="height:300px;width: 100%;" />
                </div>
                <div class="carousel-item">
                    <img src="~/images/Index/Carousel/Web_Home_TV_1900x300-liquida-medicamentos-99.png" style="height:300px;width: 100%;" />
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </section>

    <div class="">

    </div>
</section>